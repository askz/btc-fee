{
  "name": "btc-fee",
  "version": "0.0.3",
  "description": "Get recommended & custom bitcoin fees",
  "main": "lib/index.js",
  "files": [
    "README.md",
    "package.json",
    "lib"
  ],
  "scripts": {
    "build": "NODE_ENV=production babel src --out-dir lib --source-maps inline",
    "watch": "babel src --out-dir lib --watch --source-maps inline",
    "test": "NODE_PATH=src ava",
    "prepublish": "npm run build"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/*.js"
    ],
    "verbose": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genichiro/btc-fee.git"
  },
  "keywords": [
    "bitcoin",
    "btc",
    "fee"
  ],
  "author": "genichiro",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/genichiro/btc-fee/issues"
  },
  "homepage": "https://github.com/genichiro/btc-fee#readme",
  "devDependencies": {
    "ava": "^0.24.0",
    "axios-mock-adapter": "^1.10.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-register": "^6.26.0",
    "sinon": "^4.1.2"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0"
  }
}
